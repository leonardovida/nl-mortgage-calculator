# Supabase Configuration
# Copy this file to .env.local and fill in your actual values

# Your Supabase project URL (from Supabase dashboard)
NEXT_PUBLIC_SUPABASE_URL=https://your-project-id.supabase.co

# Your Supabase anonymous/public key (from Supabase dashboard > Settings > API)
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anonymous-key-here

# Optional: Service role key for admin operations (keep secure, never expose to client)
# SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# Analytics Configuration
NEXT_PUBLIC_ANALYTICS_ENABLED=true

# Environment
NODE_ENV=development

# Posthog
NEXT_PUBLIC_POSTHOG_KEY=key
NEXT_PUBLIC_POSTHOG_HOST=https://us.i.posthog.com