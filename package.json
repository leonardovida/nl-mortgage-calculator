{
  "name": "mortgage-calculator-nextjs",
  "version": "0.1.0",
  "private": true,
  "packageManager": "bun@1.1.38",
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "next build",
    "start": "next start -p 3000",
    "export": "next build && next export",
    "test": "bun test tests/",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,scss}\" \"app/**/*.{ts,tsx,css,scss}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css,scss}\" \"app/**/*.{ts,tsx,css,scss}\"",
    "typecheck": "tsc --noEmit",
    "analyze": "ANALYZE=true next build",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "supabase:reset": "supabase db reset",
    "supabase:status": "supabase status",
    "supabase:types": "supabase gen types typescript --local > src/types/supabase.ts"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.50.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.516.0",
    "next": "^14.2.30",
    "posthog-js": "^1.255.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-number-format": "^5.4.4",
    "recharts": "^2.15.4",
    "tailwind-merge": "^3.3.1",
    "typescript": "^5.8.3"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^15.3.4",
    "@next/eslint-plugin-next": "^14.2.30",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.19.1",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "autoprefixer": "^10.4.21",
    "bun-types": "^1.2.16",
    "eslint": "^8.57.1",
    "eslint-config-next": "^14.2.30",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "postcss": "^8.5.6",
    "prettier": "^3.5.3",
    "supabase": "^2.26.9",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
